{
  "name": "pecado-vue-ui",
  "version": "1.0.0",
  "description": "低代码系统",
  "author": "batizhao",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "inspect": "vue-cli-service inspect>inspect.js",
    "build:parser": "vue-cli-service build  --target lib --name parser src/components/CodeEditor/mark/index.js --mode parser"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/batizhao/pecado-vue-ui.git"
  },
  "dependencies": {
    "@d2-projects/d2-crud": "^2.1.2",
    "@riophae/vue-treeselect": "^0.4.0",
    "@types/vuedraggable": "^2.24.0",
    "axios": "^0.21.1",
    "clipboard": "2.0.4",
    "codemirror": "^5.63.1",
    "core-js": "3.6.5",
    "echarts": "^4.9.0",
    "element-ui": "^2.13.2",
    "file-saver": "2.0.2",
    "fuse.js": "^6.4.6",
    "highlight.js": "^10.6.0",
    "js-beautify": "^1.13.5",
    "js-cookie": "^2.2.1",
    "jsencrypt": "^3.1.0",
    "nprogress": "^0.2.0",
    "quill": "^1.3.7",
    "screenfull": "^5.1.0",
    "sortablejs": "^1.13.0",
    "throttle-debounce": "2.1.0",
    "vue": "^2.6.11",
    "vue-count-to": "^1.0.13",
    "vue-cropper": "^0.5.6",
    "vue-i18n": "^6.1.3",
    "vue-router": "^3.1.3",
    "vuedraggable": "2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "connect": "^3.7.0",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.7.0",
    "lint-staged": "^10.5.4",
    "runjs": "^4.4.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "svg-sprite-loader": "^5.2.1",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
